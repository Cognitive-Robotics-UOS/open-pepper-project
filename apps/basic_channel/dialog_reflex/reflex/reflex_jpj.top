#-----------------------------------------------------------
# 家電連携応答ボイス
#-----------------------------------------------------------
concept:(remocon_response) ["\rspd=110\\vct=150\はいっ、"]



#The reflex will be different depand on Pepper's pleasure level

#Pepper will try to run cocorogumi animation use B2C/LaunchCgmy event.
#Cocorogumi will only show at the first time of greeting everyday.The judge process is in cocorogumi app.

u: (~reflex_robotName)
    #^runTag(DialogReflex/RobotName)
    ^first[
    "$EEU/Pleasure==2 ~greetings_callpepper_high"
    "$EEU/Pleasure==1 ~greetings_callpepper_low"
    "$EEU/Pleasure==0 ~greetings_callpepper_low"
    "~greetings_callpepper_low"
    ]

    $B2C/LaunchCgmy="0_-1"
    $DialogReflex/UnengagedReaction="RobotName"

u: (_~reflex_greetings_morning)
    #^runTag(DialogReflex/Greetings)
    ^first[
    ##Answer back from 4am to 11am
    "
        $Launchpad/Hour>3 $Launchpad/Hour<11
        ^first[
            "$EEU/Pleasure==2 ~greetings_ohayou_high"
            "$EEU/Pleasure==1 ~greetings_ohayou_low"
            "$EEU/Pleasure==0 ~greetings_ohayou_low"
            "~greetings_ohayou_low"
        ]
        ^first[
        "
        ##Notify the rain if the weather is rainy today with a 1/5 change
            $B2C/Greeting/isRain==True
            ^rand[
                "~y_rain_goout_message $B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
            ]
        "
        "$B2C/LaunchCgmy=0_-1"
        ]
        $DialogReflex/UnengagedReaction="Greetings/Morning"
    "

    ##Ignore "good moring" greetings
    "
        $DialogReflex/IgnoreByTime="Morning"
    "
    ]

u: (_~reflex_greetings_home)
    #^runTag(DialogReflex/Greetings)
    ^first[
        "$Launchpad/Hour>14 $DialogReflex/NeedToIgnoreHome = 0"
        "$Launchpad/Hour<2 $DialogReflex/NeedToIgnoreHome = 0"
        "$DialogReflex/NeedToIgnoreHome = 1"
    ]
    ^first[
    ##Answer back from 15pm to 2am
    "
        $DialogReflex/NeedToIgnoreHome == 0
        ^first[
            "$EEU/Pleasure==2 ~greetings_comein_high"
            "$EEU/Pleasure==1 ~greetings_comein_low"
            "$EEU/Pleasure==0 ~greetings_comein_low"
            "~greetings_comein_low"
        ]
        ^first[
            "
                $B2C/Greeting/isRain==True
                ^rand[
                    "~y_rain_backhome_message $B2C/LaunchCgmy=0_-1"
                    "$B2C/LaunchCgmy=0_-1"
                    "$B2C/LaunchCgmy=0_-1"
                    "$B2C/LaunchCgmy=0_-1"
                    "$B2C/LaunchCgmy=0_-1"
                ]
            "
            "$B2C/LaunchCgmy=0_-1"
        ]
        $DialogReflex/UnengagedReaction="Greetings/Home"
    "
    ##Ignore "Home" greetings
    "
        $DialogReflex/IgnoreByTime="Home"
    "
    ]
u: (_~reflex_goodbye_night)
    #^runTag(DialogReflex/Greetings)
    ^first[
        "$Launchpad/Hour>19 $DialogReflex/NeedToIgnoreNight = 0"
        "$Launchpad/Hour<2 $DialogReflex/NeedToIgnoreNight = 0"
        "$DialogReflex/NeedToIgnoreNight = 1"
    ]
    ^first[
    ##Answer back from 20pm to 2am
    "
        $DialogReflex/NeedToIgnoreNight == 0
        ^first[
            "$EEU/Pleasure==2 ~greetings_goodnight_high"
            "$EEU/Pleasure==1 ~greetings_goodnight_low"
            "$EEU/Pleasure==0 ~greetings_goodnight_low"
            "~greetings_goodnight_low"
        ]
        $B2C/LaunchCgmy="0_-1"
        $DialogReflex/UnengagedReaction="Goodbye/Night"
    "
    ##Ignore "Night" greetings
    "
        $DialogReflex/IgnoreByTime="Night"
    "
    ]
u: (_~reflex_goodbye_leaving)
    #^runTag(DialogReflex/Greetings)
    ^first[
    ##Answer back from 4am to 11am
    "
        $Launchpad/Hour>3 $Launchpad/Hour<11
        ^first[
            "$EEU/Pleasure==2 ~greetings_goout_high"
            "$EEU/Pleasure==1 ~greetings_goout_low"
            "$EEU/Pleasure==0 ~greetings_goout_low"
            "~greetings_goout_low"
        ]
        ^first[
        ##Notify the rain if the weather is rainy today with a 1/5 change
        "
            $B2C/Greeting/isRain==True
            ^rand[
                "~y_rain_goout_message $B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
                "$B2C/LaunchCgmy=0_-1"
            ]
        "
        "$B2C/LaunchCgmy=0_-1"
        ]
        $DialogReflex/UnengagedReaction="Goodbye/leaving"
    "

    ##Ignore "leaving" greetings
    "
        $DialogReflex/IgnoreByTime="Leaving"
    "
    ]
u: (~reflex_comeHere)  # confirmation is needed
    #^runTag(DialogReflex/ComeHere)
    ^call(PackageManager.hasPackage("come-here"))
    c1:(true)
        えっ、ぼくですか
        u2: ([はい そう ええ])  # we can't use the lexicon's yes/no concepts with our limitations
            ^call(ALBasicAwareness.setEngagementMode("FullyEngaged"))
            はい、行きます
            ^pCall(ALAutonomousLife.switchFocus(\"come-here/interactive/come-here\", 256))
        u2: ([いいえ 違う いや])
            あっ、間違った

u: (~reflex_robotName ~reflex_comeHere)  # confirmation is not needed
    ^call(PackageManager.hasPackage("come-here"))
    c1:(true)
        ^call(ALBasicAwareness.setEngagementMode("FullyEngaged"))
        #^runTag(DialogReflex/RobotNameAndComeHere)
        はい、行きます
        ^pCall(ALAutonomousLife.switchFocus(\"come-here/interactive/come-here\", 256))
    #c1:(false)
        #^runTag(DialogReflex/RobotNameAndComeHere)

