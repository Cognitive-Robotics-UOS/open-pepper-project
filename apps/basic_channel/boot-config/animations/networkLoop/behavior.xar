                                                <bitmap>media/images/box/interaction/play_music.png</bitmap>
                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import time
from threading import Lock

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.player = ALProxy('ALAudioPlayer')
        self.playerStop = ALProxy('ALAudioPlayer', True) #Create another proxy as wait is blocking if audioout is remote
        self.mutex = Lock()

    def onLoad(self):
        self.mutex.acquire()
        self.id = None
        self.mutex.release()

    def onUnload(self):
        self.mutex.acquire()
        if self.id != None:
            try:
                id = self.id
                self.id = None
                time.sleep(1)
                self.playerStop.stop(id)
            except:
                pass
        self.mutex.release()

    def onInput_onStart(self, p):
        self.onUnload()
        self.mutex.acquire()
        try:
            self.id = self.player.post.playFileInLoop(p,self.getParameter("Volume (%)")/100.,self.getParameter("Balance L/R"))
        except:
            pass
        self.mutex.release()

    def onInput_onStop(self):
        self.onUnload()]]>
</content>
